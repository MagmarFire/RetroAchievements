function GetObtainablePokemon()
{
    ret = {}
    for num in pokemon
    {
        mon = pokemon[num]
        if (mon["obtainable"] == 1)
        {
            ret[num] = mon
        }
    }
    return ret
}

function NumberOfPokemonCaught()
{
    ret = 0
    for num in pokemon
    {
        mon = pokemon[num]
        ret = ret + bit(mon["bit"], mon["address"])
    }
    return ret
}

